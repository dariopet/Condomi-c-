@model WebApplication.Cheques

@{
    ViewBag.Title = "Delete";  
}

<div class="modal-header ">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button> 
    <h3>¿Está seguro de anular este cheque?</h3>
</div>


<div class="modal-body ">

<div>
   

    <dl class="dl-horizontal">
      

        <dt>
           Banco
        </dt>

        <dd>
            @Html.DisplayFor(model => model.banco)
        </dd>

        <dt>
           Fecha emitido
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fechaEmitido)
        </dd>

        <dt>
           Fecha de cobro
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fechaCobro)
        </dd>
        <dt>
           Monto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.monto)
        </dd>

        <dt>
           Nº de cheque
        </dt>

        <dd>
            @Html.DisplayFor(model => model.numCheque)
        </dd>
             
        <dt>
           Recibido de
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Terceros.nombre)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

         <div class="modal-footer">
                <button class="btn btn-warning hidden-print" data-dismiss="modal">No</button>
                <input class="btn btn-success hidden-print" type="submit" value="Si" />
            </div>
    }
    </div>
</div>

